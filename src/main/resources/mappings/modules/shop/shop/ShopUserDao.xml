<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thinkgem.jeesite.modules.shop.shop.dal.dao.ShopUserDao">
	<sql id="Base_column">
		id,phone,password,salt,name,roleid,status,createtime,version
	</sql>

   <insert id="addShopUser" >
	   insert into
	   shop_user (<include refid="Base_column"/>)
   	   values(
	      #{id},#{phone},#{password},#{salt},#{name},#{roleid},#{status},#{createtime},#{version}
	   )
   </insert>
</mapper>